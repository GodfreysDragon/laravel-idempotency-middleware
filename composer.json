{
    "name": "broken-titan/laravel-idempotency-middleware",
    "description": "Idempotency middleware for Laravel.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Marshall Miller",
            "email": "contact@brokentitan.com"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "mockery/mockery": "1.4.4",
        "phpunit/phpunit": "9.5.10",
        "orchestra/testbench": "^6.24"
    },    
    "extra": {
        "laravel": {
            "providers": [
                "BrokenTitan\\Idempotency\\Providers\\IdempotencyServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "BrokenTitan\\Idempotency\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "test": [
            "vendor\\bin\\phpunit"
        ]
    }
}
